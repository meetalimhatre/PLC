        "ProjectSurchargesAccountGroupsOverlappingMessageModel": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string",
                    "enum": [
                        "PROJECT_SURCHARGES_ACCOUNT_GROUPS_OVERLAPPING_WARNING"
                    ],
                    "default": "PROJECT_SURCHARGES_ACCOUNT_GROUPS_OVERLAPPING_WARNING",
                    "description": "If surcharges are defined for different account groups which contain the same account more than once"
                },
                "details": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "ACCOUNT_GROUPS_WITH_OVERLAPS": {
                                "type": "array",
                                "items": {
                                    "type": "integer"
                                },
                                "description": "Set of account group ids that sharing at least one account. Example [100, 200] => groups with id 100 and 200 have overlaps; [100, 200, 300] => groups with id 100, 200 and 300 have overlaps"
                            }
                        }
                    }
                },
                "severity": {
                    "type": "string",
                    "enum": [
                        "Warning"
                    ],
                    "default": "Warning"
                }
            }
