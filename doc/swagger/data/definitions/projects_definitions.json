        "LifecyclePeriodModel": {
            "type": "object",
            "required": [
                "LIFECYCLE_PERIOD_FROM",
                "VALUE"
            ],
            "properties": {
                "LIFECYCLE_PERIOD_FROM": {
                    "type": "integer",
                    "format": "int32",
                    "description": "Number of months from 1900-01-01 when the lifecycle period starts"
                },
                "LIFECYCLE_PERIOD_FROM_DATE": {
                    "type": "string",
                    "format": "date",
                    "description": "Representation of the lifecycle period start as date"
                },
                "VALUE": {
                    "type": "string",
                    "description": "decimal number escaped as string; can only be != 0; if objects send with VALUE == 0 this leads to validation errors"
                }
            }
        },
