{
  "xsappname": "xsac-plc",
  "tenant-mode": "shared",
  "scopes": [
    {
      "name": "$XSAPPNAME.Callback",
      "description": "With this scope, the callbacks from the SaaS Provisioning service (formerly known as tenant-onboarding): onboarding, offboarding and getDependencies can be called.",
      "grant-as-authority-to-apps": [
        "$XSAPPNAME(application,sap-provisioning,tenant-onboarding)"
      ]
    },
    {
      "name": "$XSAPPNAME.ApiDocR",
      "description": "Read access for the swagger api documentation"
    },
    {
      "name": "$XSAPPNAME.AnalyticsViewer",
      "description": "Read all calculation views"
    },
    {
      "name": "$XSAPPNAME.Base",
      "description": "Basic access for init-session and ping"
    },
    {
      "name": "$XSAPPNAME.CalcVerOpen",
      "description": "Read all calculation versions, i.e. open them"
    },
    {
      "name": "$XSAPPNAME.CFAdmE",
      "description": "Modify tenant information, create HDI, deploy artifacts, run post-install. Is intended for operators and not grantable to customers"
    },
    {
      "name": "$XSAPPNAME.MdrLogs",
      "description": "Read master data replication logs table"
    },
    {
      "name": "$XSAPPNAME.MdrStatistics",
      "description": "Read master data replication statistics table"
    },
    {
      "name": "$XSAPPNAME.Repl",
      "description": "configure replication tool"
    },
    {
      "name": "$XSAPPNAME.Sched",
      "description": "configure internal scheduling tool"
    },
    {
        "name": "$XSAPPNAME.RetPeriodsR",
        "description": "read retention periods for user, customer, vendor, project"
    },
    {
        "name": "$XSAPPNAME.RetPeriodsE",
        "description": "create, update and delete retention periods for user, customer, vendor, project"
    }
  ],
  "foreign-scope-references": [
    "$ACCEPT_GRANTED_SCOPES"
  ],
  "authorities": [
    "$ACCEPT_GRANTED_AUTHORITIES",
    "$XSAPPNAME.CFAdmE"
  ],
