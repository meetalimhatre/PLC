VIEW "sap.plc.db.replication.views::v_cbpr" ( "KOKRS", "PRZNR", "VKSTA" ) AS select CBPR.KOKRS, CBPR.PRZNR, CBPR.VKSTA
                from "sap.plc.db::repl.cbpr" AS CBPR
                	WHERE CBPR.DATBI = ( select MAX(DATBI) 
                								from "sap.plc.db::repl.cbpr" AS CBPR1
                								WHERE CBPR1.KOKRS = CBPR.KOKRS AND 
                									  CBPR1.PRZNR = CBPR.PRZNR  )
                	  AND CBPR.DATBI >= TO_DATS(CURRENT_DATE)