COLUMN TABLE "sap.plc.db::basis.t_project_lifecycle_period_type" (
  PROJECT_ID NVARCHAR(35) NOT NULL,
  "YEAR" INTEGER NOT NULL,
  IS_YEAR_SELECTED TINYINT,
  PERIOD_TYPE NVARCHAR(20) NOT NULL DEFAULT 'YEARLY',
  LAST_MODIFIED_ON TIMESTAMP NOT NULL,
  LAST_MODIFIED_BY NVARCHAR(256) NOT NULL,
  PRIMARY KEY(PROJECT_ID, "YEAR")
)