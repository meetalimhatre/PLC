COLUMN TABLE "sap.plc.db::basis.t_company_code" (
  COMPANY_CODE_ID NVARCHAR(4) NOT NULL,
  CONTROLLING_AREA_ID NVARCHAR(4) NOT NULL,
  COMPANY_CODE_CURRENCY_ID NVARCHAR(3),
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  DELETED_FROM_SOURCE TINYINT,
  PRIMARY KEY(COMPANY_CODE_ID, _VALID_FROM)
)