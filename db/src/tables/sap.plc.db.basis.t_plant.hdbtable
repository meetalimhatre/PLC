COLUMN TABLE "sap.plc.db::basis.t_plant" (
  PLANT_ID NVARCHAR(8) NOT NULL,
  COMPANY_CODE_ID NVARCHAR(4) NOT NULL,
  COUNTRY NVARCHAR(100),
  POSTAL_CODE NVARCHAR(35),
  REGION NVARCHAR(100),
  CITY NVARCHAR(100),
  STREET_NUMBER_OR_PO_BOX NVARCHAR(100),
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  DELETED_FROM_SOURCE TINYINT,
  PRIMARY KEY(PLANT_ID, _VALID_FROM)
)