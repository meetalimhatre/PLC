COLUMN TABLE "sap.plc.db::basis.t_item_category" (
  ITEM_CATEGORY_ID INTEGER NOT NULL,
  DISPLAY_ORDER INTEGER NOT NULL,
  CHILD_ITEM_CATEGORY_ID INTEGER NOT NULL DEFAULT 0,
  ICON NVARCHAR(10) NOT NULL DEFAULT 'dummy',
  CREATED_ON TIMESTAMP,
  CREATED_BY NVARCHAR(256),
  LAST_MODIFIED_ON TIMESTAMP,
  LAST_MODIFIED_BY NVARCHAR(256),
  ITEM_CATEGORY_CODE NVARCHAR(25) NOT NULL DEFAULT 'dummy',
  PRIMARY KEY(ITEM_CATEGORY_ID, CHILD_ITEM_CATEGORY_ID)
)