COLUMN TABLE "sap.plc.db::basis.t_overhead_group" (
  OVERHEAD_GROUP_ID NVARCHAR(10) NOT NULL,
  PLANT_ID NVARCHAR(8) NOT NULL,
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  DELETED_FROM_SOURCE TINYINT,
  PRIMARY KEY(OVERHEAD_GROUP_ID, PLANT_ID, _VALID_FROM)
)