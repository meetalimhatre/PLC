COLUMN TABLE "sap.plc.db::basis.t_project_monthly_lifecycle_period" (
  PROJECT_ID NVARCHAR(35) NOT NULL,
  "YEAR" INTEGER NOT NULL,
  SELECTED_MONTH INTEGER NOT NULL,
  MONTH_DESCRIPTION NVARCHAR(100),
  LAST_MODIFIED_ON TIMESTAMP NOT NULL,
  LAST_MODIFIED_BY NVARCHAR(256) NOT NULL,
  PRIMARY KEY(PROJECT_ID, "YEAR", SELECTED_MONTH)
)