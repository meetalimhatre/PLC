COLUMN TABLE "sap.plc.db::basis.t_costing_sheet_overhead" (
  COSTING_SHEET_OVERHEAD_ID INTEGER NOT NULL,
  CREDIT_ACCOUNT_ID NVARCHAR(10),
  CREDIT_FIXED_COST_PORTION INTEGER,
  IS_ROLLED_UP TINYINT NOT NULL,
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  USE_DEFAULT_FIXED_COST_PORTION TINYINT NOT NULL DEFAULT 1,
  PRIMARY KEY(COSTING_SHEET_OVERHEAD_ID, _VALID_FROM)
)