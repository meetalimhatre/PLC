  BUSINESS_OBJECT NVARCHAR(127) NOT NULL,
  COLUMN_ID NVARCHAR(127) NOT NULL,
  IS_CUSTOM TINYINT NOT NULL DEFAULT 0,
  ROLLUP_TYPE_ID INTEGER DEFAULT 0,
