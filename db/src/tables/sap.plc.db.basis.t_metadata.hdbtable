  REF_UOM_CURRENCY_PATH NVARCHAR(127),
  REF_UOM_CURRENCY_BUSINESS_OBJECT NVARCHAR(127),
  REF_UOM_CURRENCY_COLUMN_ID NVARCHAR(127),
  UOM_CURRENCY_FLAG TINYINT,
  SEMANTIC_DATA_TYPE NVARCHAR(35) NOT NULL,
  SEMANTIC_DATA_TYPE_ATTRIBUTES NVARCHAR(250),
  VALIDATION_REGEX_ID NVARCHAR(30),
  PROPERTY_TYPE INTEGER,
  IS_IMMUTABLE_AFTER_SAVE TINYINT,
  IS_REQUIRED_IN_MASTERDATA TINYINT,
  IS_WILDCARD_ALLOWED TINYINT,
  IS_USABLE_IN_FORMULA TINYINT,
  RESOURCE_KEY_DISPLAY_NAME NVARCHAR(256),
  RESOURCE_KEY_DISPLAY_DESCRIPTION NVARCHAR(256),
