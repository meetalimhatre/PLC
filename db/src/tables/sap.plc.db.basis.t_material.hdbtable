COLUMN TABLE "sap.plc.db::basis.t_material" (
  MATERIAL_ID NVARCHAR(40) NOT NULL,
  BASE_UOM_ID NVARCHAR(3),
  MATERIAL_GROUP_ID NVARCHAR(9),
  MATERIAL_TYPE_ID NVARCHAR(4),
  IS_CREATED_VIA_CAD_INTEGRATION TINYINT,
  IS_PHANTOM_MATERIAL TINYINT,
  IS_CONFIGURABLE_MATERIAL TINYINT,
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  DELETED_FROM_SOURCE TINYINT,
  PRIMARY KEY(MATERIAL_ID, _VALID_FROM)
)