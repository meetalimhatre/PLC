COLUMN TABLE "sap.plc.db::basis.t_variant_item_temporary" (
  VARIANT_ID INTEGER NOT NULL,
  ITEM_ID INTEGER NOT NULL,
  CALCULATION_VERSION_ID INTEGER NOT NULL,
  IS_INCLUDED TINYINT NOT NULL DEFAULT 0,
  QUANTITY DECIMAL(28, 7),
  QUANTITY_CALCULATED DECIMAL(28, 7),
  QUANTITY_STATE SMALLINT DEFAULT 2,
  QUANTITY_UOM_ID NVARCHAR(3),
  TOTAL_QUANTITY DECIMAL(28, 7),
  TOTAL_COST DECIMAL(28, 7),
  PRIMARY KEY(VARIANT_ID, ITEM_ID, CALCULATION_VERSION_ID)
)