COLUMN TABLE "sap.plc.db::basis.t_material_account_determination" (
  CONTROLLING_AREA_ID NVARCHAR(4) NOT NULL,
  MATERIAL_TYPE_ID NVARCHAR(4) NOT NULL,
  PLANT_ID NVARCHAR(8) NOT NULL,
  VALUATION_CLASS_ID NVARCHAR(4) NOT NULL,
  ACCOUNT_ID NVARCHAR(10) NOT NULL,
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  DELETED_FROM_SOURCE TINYINT,
  PRIMARY KEY(CONTROLLING_AREA_ID, MATERIAL_TYPE_ID, PLANT_ID, VALUATION_CLASS_ID, _VALID_FROM)
)