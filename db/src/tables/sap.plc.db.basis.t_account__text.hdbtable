COLUMN TABLE "sap.plc.db::basis.t_account__text" (
  ACCOUNT_ID NVARCHAR(10) NOT NULL,
  CONTROLLING_AREA_ID NVARCHAR(4) NOT NULL,
  "LANGUAGE" NVARCHAR(11) NOT NULL,
  ACCOUNT_DESCRIPTION NVARCHAR(250),
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  PRIMARY KEY(ACCOUNT_ID, CONTROLLING_AREA_ID, "LANGUAGE", _VALID_FROM)
)