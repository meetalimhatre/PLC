COLUMN TABLE "sap.plc.db::basis.t_uom__text" (
  UOM_ID NVARCHAR(3) NOT NULL,
  "LANGUAGE" NVARCHAR(11) NOT NULL,
  UOM_CODE NVARCHAR(5),
  UOM_DESCRIPTION NVARCHAR(40),
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  PRIMARY KEY(UOM_ID, "LANGUAGE", _VALID_FROM)
)