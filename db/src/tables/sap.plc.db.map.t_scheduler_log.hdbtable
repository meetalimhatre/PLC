COLUMN TABLE "sap.plc.db::map.t_scheduler_log" (
  RUN_ID NVARCHAR(120) NOT NULL,
  SCHED_NAME NVARCHAR(120) NOT NULL,
  JOB_NAME NVARCHAR(200) NOT NULL,
  JOB_GROUP NVARCHAR(200) NOT NULL,
  STATE NVARCHAR(20) NOT NULL,
  FIRED_TIME TIMESTAMP NOT NULL,
  FINISHED_TIME TIMESTAMP DEFAULT NULL,
  PRIMARY KEY(RUN_ID)
)