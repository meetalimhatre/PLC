COLUMN TABLE "sap.plc.db::basis.t_default_settings" (
  USER_ID NVARCHAR(256) NOT NULL,
  CONTROLLING_AREA_ID NVARCHAR(4),
  COMPANY_CODE_ID NVARCHAR(4),
  PLANT_ID NVARCHAR(8),
  REPORT_CURRENCY_ID NVARCHAR(3),
  COMPONENT_SPLIT_ID NVARCHAR(35),
  COSTING_SHEET_ID NVARCHAR(15),
  LIFECYCLE_PERIOD_INTERVAL TINYINT NOT NULL DEFAULT 12,
  PRIMARY KEY(USER_ID)
)