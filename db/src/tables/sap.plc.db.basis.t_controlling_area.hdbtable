COLUMN TABLE "sap.plc.db::basis.t_controlling_area" (
  CONTROLLING_AREA_ID NVARCHAR(4) NOT NULL,
  CONTROLLING_AREA_CURRENCY_ID NVARCHAR(3),
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  DELETED_FROM_SOURCE TINYINT,
  PRIMARY KEY(CONTROLLING_AREA_ID, _VALID_FROM)
)