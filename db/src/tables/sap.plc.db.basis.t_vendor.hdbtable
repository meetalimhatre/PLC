COLUMN TABLE "sap.plc.db::basis.t_vendor" (
  VENDOR_ID NVARCHAR(10) NOT NULL,
  VENDOR_NAME NVARCHAR(100),
  COUNTRY NVARCHAR(100),
  POSTAL_CODE NVARCHAR(35),
  REGION NVARCHAR(100),
  CITY NVARCHAR(100),
  STREET_NUMBER_OR_PO_BOX NVARCHAR(100),
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  DELETED_FROM_SOURCE TINYINT,
  IS_PERSONAL_DATA TINYINT DEFAULT 0,
  PRIMARY KEY(VENDOR_ID, _VALID_FROM)
)