COLUMN TABLE "sap.plc.db::basis.t_costing_sheet__text" (
  COSTING_SHEET_ID NVARCHAR(15) NOT NULL,
  "LANGUAGE" NVARCHAR(11) NOT NULL,
  COSTING_SHEET_DESCRIPTION NVARCHAR(250),
  TOTAL_COST2_DESCRIPTION NVARCHAR(50),
  TOTAL_COST3_DESCRIPTION NVARCHAR(50),
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  PRIMARY KEY(COSTING_SHEET_ID, "LANGUAGE", _VALID_FROM)
)