COLUMN TABLE "sap.plc.db::basis.t_one_time_product_cost" (
  ONE_TIME_COST_ID INTEGER NOT NULL,
  CALCULATION_ID INTEGER NOT NULL,
  COST_TO_DISTRIBUTE DECIMAL(28, 7),
  COST_NOT_DISTRIBUTED DECIMAL(28, 7),
  DISTRIBUTION_TYPE TINYINT NOT NULL DEFAULT 0,
  LAST_MODIFIED_ON TIMESTAMP NOT NULL,
  LAST_MODIFIED_BY NVARCHAR(256) NOT NULL,
  PRIMARY KEY(ONE_TIME_COST_ID, CALCULATION_ID)
)