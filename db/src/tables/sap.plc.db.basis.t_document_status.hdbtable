COLUMN TABLE "sap.plc.db::basis.t_document_status" (
  DOCUMENT_TYPE_ID NVARCHAR(3) NOT NULL,
  DOCUMENT_STATUS_ID NVARCHAR(2) NOT NULL,
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  DELETED_FROM_SOURCE TINYINT,
  PRIMARY KEY(DOCUMENT_TYPE_ID, DOCUMENT_STATUS_ID, _VALID_FROM)
)