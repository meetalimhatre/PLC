COLUMN TABLE "sap.plc.db::basis.t_account" (
  ACCOUNT_ID NVARCHAR(10) NOT NULL,
  CONTROLLING_AREA_ID NVARCHAR(4) NOT NULL,
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  DELETED_FROM_SOURCE TINYINT,
  PRIMARY KEY(ACCOUNT_ID, CONTROLLING_AREA_ID, _VALID_FROM)
)