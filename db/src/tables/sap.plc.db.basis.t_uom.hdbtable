COLUMN TABLE "sap.plc.db::basis.t_uom" (
  UOM_ID NVARCHAR(3) NOT NULL,
  DIMENSION_ID NVARCHAR(10) NOT NULL,
  NUMERATOR INTEGER NOT NULL,
  DENOMINATOR INTEGER NOT NULL,
  EXPONENT_BASE10 SMALLINT NOT NULL,
  SI_CONSTANT DECIMAL(9, 6) NOT NULL,
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  DELETED_FROM_SOURCE TINYINT,
  PRIMARY KEY(UOM_ID, _VALID_FROM)
)