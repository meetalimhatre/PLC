COLUMN TABLE "sap.plc.db::basis.t_material_plant" (
  MATERIAL_ID NVARCHAR(40) NOT NULL,
  PLANT_ID NVARCHAR(8) NOT NULL,
  OVERHEAD_GROUP_ID NVARCHAR(10),
  VALUATION_CLASS_ID NVARCHAR(4),
  MATERIAL_LOT_SIZE DECIMAL(28, 7),
  MATERIAL_LOT_SIZE_UOM_ID NVARCHAR(3),
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  DELETED_FROM_SOURCE TINYINT,
  PRIMARY KEY(MATERIAL_ID, PLANT_ID, _VALID_FROM)
)