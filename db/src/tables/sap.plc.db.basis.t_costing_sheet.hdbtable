COLUMN TABLE "sap.plc.db::basis.t_costing_sheet" (
  COSTING_SHEET_ID NVARCHAR(15) NOT NULL,
  CONTROLLING_AREA_ID NVARCHAR(4) NOT NULL,
  IS_TOTAL_COST2_ENABLED TINYINT NOT NULL DEFAULT 0,
  IS_TOTAL_COST3_ENABLED TINYINT NOT NULL DEFAULT 0,
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  PRIMARY KEY(COSTING_SHEET_ID, _VALID_FROM)
)