COLUMN TABLE "sap.plc.db::basis.t_price_source" (
  PRICE_SOURCE_ID NVARCHAR(20) NOT NULL,
  PRICE_SOURCE_TYPE_ID TINYINT NOT NULL DEFAULT 1,
  CONFIDENCE_LEVEL_ID INTEGER,
  DETERMINATION_SEQUENCE TINYINT NOT NULL DEFAULT 0,
  CREATED_ON TIMESTAMP,
  CREATED_BY NVARCHAR(256),
  LAST_MODIFIED_ON TIMESTAMP,
  LAST_MODIFIED_BY NVARCHAR(256),
  PRIMARY KEY(PRICE_SOURCE_ID, PRICE_SOURCE_TYPE_ID)
)