COLUMN TABLE "sap.plc.db::basis.t_currency__text" (
  CURRENCY_ID NVARCHAR(3) NOT NULL,
  "LANGUAGE" NVARCHAR(11) NOT NULL,
  CURRENCY_CODE NVARCHAR(5),
  CURRENCY_DESCRIPTION NVARCHAR(40),
  _VALID_FROM TIMESTAMP NOT NULL,
  _VALID_TO TIMESTAMP,
  _SOURCE TINYINT,
  _CREATED_BY NVARCHAR(256),
  PRIMARY KEY(CURRENCY_ID, "LANGUAGE", _VALID_FROM)
)