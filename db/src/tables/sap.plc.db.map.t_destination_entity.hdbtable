COLUMN TABLE "sap.plc.db::map.t_destination_entity" (
  ID INTEGER NOT NULL,
  TABLE_NAME NVARCHAR(128) NOT NULL,
  LABEL NVARCHAR(128) NOT NULL,
  DESCRIPTION NVARCHAR(5000),
  INPUT_SQL_DEFAULT NVARCHAR(5000),
  INPUT_SQL NCLOB,
  IS_REPL_MANDATORY TINYINT,
  REPL_STATUS NVARCHAR(8),
  PRIMARY KEY(ID)
)